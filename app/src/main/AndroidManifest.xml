<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.faceflag.android">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <application android:allowBackup="true"
        android:label="@string/app_name"
        android:icon="@drawable/ic_media_route_on_mono_dark"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <meta-data android:name="com.google.android.gms.vision.DEPENDENCIES" android:value="face"/>

        <activity
            android:name=".PhotoActivity"
            android:icon="@drawable/common_signin_btn_icon_focus_light">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>

        <activity
            android:name=".FlagDisplayActivity"
            android:icon="@drawable/common_signin_btn_icon_focus_light">
            android:parentActivityName=".PhotoActivity">

        </activity>

        <activity
            android:name=".PhotoDisplayActivity"
            android:parentActivityName=".PhotoActivity"
            android:icon="@drawable/common_signin_btn_icon_focus_light">
        </activity>

        <activity
            android:name=".FinalImageActivity"
            android:icon="@drawable/common_signin_btn_icon_focus_light">
            android:parentActivityName=".FlagDisplayActivity">
        </activity>

    </application>


</manifest>
